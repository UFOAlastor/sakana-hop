  "description": "sakana hop",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sakana.helloWorld"
  ],
  "main": "./out/extension.js",
  "keywords": [
      "石蒜",
      "莉可丽丝",
      "Lycoris Recoil",
      "Rikorisu Rikoiru",
      "锦木千束",
      "井上泷奈",
      "立牌",
      "KADOKAWA"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sakana.helloWorld",
        "title": "Hello World"
      }
    ]
